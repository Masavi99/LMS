@model int
@{
    ViewData["Title"] = "ButtonPartial";
}

<td>
    <div class="btn-group">
        <a asp-action="Edit" class="btn btn-primary" asp-route-id="@Model" title="Edit"><i class="far fa-edit"></i></a>
        <a asp-action="Details" class="btn btn-success" asp-route-id="@Model" title="Details"><i class="fas fa-list"></i></a>
        <a asp-action="Delete" class="btn btn-danger" asp-route-id="@Model" title="Delete"><i class="fas fa-trash"></i></a>
    </div>
</td>

